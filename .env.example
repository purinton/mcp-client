# Example environment variables for mcp-client
# Copy this file to .env and fill in your values

# MCP server URL (default: http://localhost:1234/)
MCP_URL=http://localhost:1234/

# MCP server port (default: 1234)
MCP_PORT=1234

# Authentication token for MCP server
MCP_TOKEN=your-mcp-token-here

# Optional: Reconnect delay settings (in milliseconds)
MCP_RECONNECT_BASE_DELAY=1000
MCP_RECONNECT_MAX_DELAY=60000
